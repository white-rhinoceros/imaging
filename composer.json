{
    "name": "white-rhinoceros/imaging",
    "description": "Laravel package for manipulating images",
    "type": "library",
    "keywords": ["laravel", "image"],
    "license": "MIT",
    "version": "0.1.1",
    "authors": [
        {
            "name": "Denis Maysky"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-mbstring": "*",
        "ext-imagick": "*"
    },
    "autoload": {
        "psr-4": {
            "Whiterhino\\Imaging\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Whiterhino\\Imaging\\ImagingServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Whiterhino\\Imaging\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint": "bash tools/lint.sh",
        "test": "vendor/bin/phpunit",
        "test:coverage": "XDEBUG_MODE=coverage vendor/bin/phpunit --configuration=phpunit.coverage.xml.dist"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
